{
  "name": "graph-server",
  "version": "1.0.0",
  "description": "Hosts various information on all public transport in the island",
  "main": "Server.js",
  "directories": {
    "test": "test"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/transit-app"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "nyc mocha --timeout 10000 \"./api/test\" -recursive --exit",
    "start": "node ./bin/www",
    "dev": "concurrently \"nodemon --inspect ../api/bin/www\" \" cd transit-app && SET PORT=3001 && yarn start\"",
    "build": "cd transit-app && yarn build && cd .. && xcopy /e/h/y/q \"transit-app/build\" \"react-app\\\" > nul 2>&1 && del \"react-app\\app.html\" > nul 2>&1 && ren \"react-app\\index.html\" \"app.html\"",
    "ubuntu-dev": "concurrently \"nodemon --inspect api/bin/www\" \" cd transit-app && PORT=3001 yarn start\""
  },
  "nodemonConfig": {
    "ignore": [
      "./transit-app/"
    ]
  },
  "author": "Daniel Nelson",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^25.5.4",
    "mocha": "^6.2.0",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0"
  },
  "dependencies": {
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "graphql": "^15.0.0",
    "http": "0.0.1-security",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.20",
    "morgan": "~1.9.1",
    "mysql2": "2.1.0",
    "neo4j-driver": "^4.0.2",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.5.1",
    "pug": "^2.0.4",
    "react-google-button": "^0.7.1",
    "uuid": "^8.3.0"
  }
}
